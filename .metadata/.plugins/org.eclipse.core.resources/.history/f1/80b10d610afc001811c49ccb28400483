package mapaProjecao;

/**
 * Lida com a abstração de Fabricas Abstratas. Na implementação foi convertida para
 * evitar ambiguidades de chamadas provenientes do mesmo tipo herdado/implementado.
 */
public interface FabricaAbstrata {

	// Método fabricador de Mapa
	public MapaProjecao criarMapaProjecao(float x, float y);

	// Método de retorno da instância
	public FabricaAbstrata instancia();

}
